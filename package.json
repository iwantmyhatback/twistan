{
	"name": "app",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
		"preview": "npm run build && wrangler pages dev ./dist --kv CONTACT_SUBMISSIONS",
